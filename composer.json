{
    "name": "spartan/router",
    "description": "Spartan Router library",
    "type": "library",
    "keywords": [
        "library",
        "spartan",
        "router"
    ],
    "homepage": "https://spartanphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Iulian N.",
            "email": "iulian@spartanphp.com",
            "homepage": "https://spartanphp.com",
            "role": "developer"
        }
    ],
    "support": {
        "email": "support@spartanphp.com",
        "docs": "https://docs.spartanphp.com"
    },
    "require": {
        "php": ">=7.4",
        "nikic/fast-route": "^1.3",
        "spartan/adr": "^0.1.0",
        "spartan/cache": "^0.1.0",
        "spartan/http": "^0.1.0",
        "spartan/service": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Spartan\\Router\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spartan\\Router\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
